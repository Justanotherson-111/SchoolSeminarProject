{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "JWT": {
    "Secret": "YOUR_SUPER_SECRET_KEY_HERE_CHANGE_IT",
    "AccessTokenExpirationMinutes": 15,
    "RefreshTokenExpirationDays": 7
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=postgres;Port=5432;Database=yourdb;Username=youruser;Password=yourpassword"
  },
  "RateLimiting": {
    "GeneralRules": [
      {
        "Endpoint": "*:/api/auth/*",
        "Period": "1m",
        "Limit": 10
      },
      {
        "Endpoint": "*:/api/image/*",
        "Period": "1m",
        "Limit": 5
      }
    ]
  }
}
